{
  "name": "acm_meter_web",
  "version": "1.0.0",
  "description": "ACM Meter Web",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node config/dev-server.js",
    "dev:dash": "cross-env NODE_ENV=development webpack-dashboard -- node config/dev-server.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel/register --require ./tests/testRunner.js --recursive ./tests",
    "test:watch": "npm test -- --watch",
    "dll": "cross-env NODE_ENV=development webpack --config config/webpack.dll.config.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js",
    "deploy": "npm run dll && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raynxxx/ACM-Meter-Web.git"
  },
  "keywords": [
    "React"
  ],
  "author": "Rayn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Raynxxx/ACM-Meter-Web/issues"
  },
  "homepage": "https://github.com/Raynxxx/ACM-Meter-Web#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.14.0",
